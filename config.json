            "step2" : {
                "MACHINE" : "genericx86",
                "BBTARGETS" : "core-image-sato core-image-sato-dev core-image-sato-sdk core-image-minimal core-image-minimal-dev core-image-sato-sdk-ptest"
            },
            "step3" : {
